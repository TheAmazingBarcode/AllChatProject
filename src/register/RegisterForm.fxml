<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="230.0" prefWidth="640.0" stylesheets="@visualCandy/RegisterStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="register.RegisterController">
   <children>
      <TextField fx:id="username" layoutX="253.0" layoutY="90.0" prefHeight="24.0" prefWidth="149.0" styleClass="textField" />
      <Label layoutX="152.0" layoutY="95.0" text="Username" />
      <Label layoutX="152.0" layoutY="143.0" text="Password" />
      <PasswordField fx:id="password" layoutX="253.0" layoutY="138.0" prefHeight="24.0" prefWidth="149.0" styleClass="passwordField" />
      <Button fx:id="registerButton" layoutX="253.0" layoutY="180.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="25.0" prefWidth="149.0" text="Register" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="11.0" mnemonicParsing="false" onAction="#goBack" text="BACK" />
   </children>
</AnchorPane>
